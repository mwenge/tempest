     1                                          .SBTTL  MESSAGE MACROS
     2+                                         .INCLUDE ASCVG
     3+                                 .SBTTL  .ASCVG - VG MODIFIED 'ASCII'
     4+
     5+                                 ;	PROGRAMMER: RICH MOORE
     6+
     7+                                 ;
     8+                                 ;MACRO .ASCVG CONVERTS STRING OF ALPHANUMERICS TO OFFSET VALUES INTO
     9+                                 ;VGMSGA TABLE (TABLE OF VG CHARACTER JSRL'S)
    10+                                 ;
    11+                                         .MACRO .ASCVG .STRING
    12+                                         .NCHR ..N,<.STRING>
    13+                                         .IRPC ..C,<.STRING>
    14+                                         .IF EQ,''..C-^H20
    15+                                         ..X=0                   ;BLANK CHAR=0
    16+                                         .IFF
    17+                                         .IF GT,''..C-^H40
    18+                                         ..X=''..C-^H36          ;ALPHA'S ^B TO ^25
    19+                                         .IFF
    20+                                         ..X=''..C-^H2F          ;NUMBERS 1 TO ^A
    21+                                         .ENDC
    22+                                         .ENDC
    23+                                         ..X=..X*2               ;..X=VGMSGA TABLE OFFSET
    24+                                         ..N=..N-1
    25+                                         .IIF EQ,..N,..X=..X!^H80        ;SET LAST CHAR OF MSG FLAG
    26+                                         .BYTE ..X
    27+                                         .ENDR
    28+                                         .ENDM
    29+                                         .LIST   MEB
    30+                                         .NLIST  BYT
    31                                          .MACRO ASCVH ...AR1,...AR2
    32                                          .NARG ...NUM
    33                                          .IF EQ ...NUM-1
    34                                          .NCHR ...HLO,<...AR1>   ;1 ARGUMENT, CENTER STRING
    35                                          .BYTE -<...HLO*3>
    36                                          .ASCVG <...AR1>
    37                                          .IFF
    38                                          .BYTE ...AR1            ;2 ARGUMENTS: HORIZ LOC.
    39                                          .ASCVG <...AR2>         ;STRING
    40                                          .ENDC
    41                                          .ENDM
